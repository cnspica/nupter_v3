<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="fill_parent"
             android:layout_height="fill_parent"
             android:layout_marginLeft="10dp"
             android:layout_marginRight="10dp">
    <LinearLayout
            android:layout_height="17dp"
            android:layout_width="17dp"
            android:background="#D3D3D3"/>
    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_marginLeft="17dp"
            android:layout_height="18dp"
            android:paddingBottom="1dp"
            android:paddingLeft="1dp"
            android:background="@color/white"
            android:orientation="horizontal">
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginRight="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="一"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginRight="0.5dp"
                android:layout_marginLeft="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="二"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginRight="0.5dp"
                android:layout_marginLeft="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="三"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_marginRight="0.5dp"
                android:layout_marginLeft="0.5dp"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="四"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_weight="1"
                android:layout_marginLeft="0.5dp"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="五"/>

    </LinearLayout>
    <LinearLayout
            android:layout_width="18dp"
            android:layout_marginTop="17dp"
            android:paddingTop="1dp"
            android:paddingRight="1dp"
            android:orientation="vertical"
            android:background="@color/white"
            android:layout_height="fill_parent">
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="1"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="2"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="3"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="4"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="5"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="6"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="7"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="8"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="9"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="10"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_marginBottom="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="11"/>
        <TextView
                android:layout_height="fill_parent"
                android:layout_width="fill_parent"
                android:layout_marginTop="0.5dp"
                android:layout_weight="1"
                android:gravity="center"
                android:background="#D3D3D3"
                android:textColor="#94A0A4"
                android:text="12"/>
    </LinearLayout>
    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="18dp"
            android:layout_marginLeft="18dp"
            android:id="@+id/background"/>
    <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginTop="17dp"
            android:layout_marginLeft="17dp"
            android:paddingTop="1dp"
            android:paddingLeft="1dp"
            android:orientation="vertical">
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:orientation="vertical">
            <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="4"
                    android:orientation="vertical">
                <!--第一二节-->
                <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="3"
                        android:orientation="horizontal">
                    <LinearLayout
                            android:id="@+id/one_1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"/>
                    <LinearLayout
                            android:id="@+id/one_2"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"/>
                    <LinearLayout
                            android:id="@+id/one_3"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"/>
                    <LinearLayout
                            android:id="@+id/one_4"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"/>
                    <LinearLayout
                            android:id="@+id/one_5"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="vertical"/>
                </LinearLayout>
                <!--第三四五节-->
                <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="2"
                        android:orientation="horizontal">
                    <LinearLayout
                            android:id="@+id/two_1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"/>
                    <LinearLayout
                            android:id="@+id/two_2"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"/>
                    <LinearLayout
                            android:id="@+id/two_3"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"/>
                    <LinearLayout
                            android:id="@+id/two_4"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1">
                    </LinearLayout>
                    <LinearLayout
                            android:id="@+id/two_5"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"/>
                </LinearLayout>
            </LinearLayout>
            <FrameLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="5">
                <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">
                    <!--第六七节-->
                    <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                        <LinearLayout
                                android:id="@+id/three_1"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/three_2"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/three_3"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/three_4"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/three_5"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                    </LinearLayout>
                    <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="3"/>
                </LinearLayout>
                <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:orientation="vertical">
                    <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"/>
                    <!--第八九节-->
                    <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:orientation="horizontal">
                        <LinearLayout
                                android:id="@+id/four_1"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/four_2"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/four_3"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/four_4"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                        <LinearLayout
                                android:id="@+id/four_5"
                                android:layout_width="fill_parent"
                                android:layout_height="fill_parent"
                                android:layout_weight="1"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
        <!--第十、十一、十二节-->
        <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="3"
                android:orientation="horizontal">
            <LinearLayout
                    android:id="@+id/five_1"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"/>
            <LinearLayout
                    android:id="@+id/five_2"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"/>
            <LinearLayout
                    android:id="@+id/five_3"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"/>
            <LinearLayout
                    android:id="@+id/five_4"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"/>
            <LinearLayout
                    android:id="@+id/five_5"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_weight="1"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>